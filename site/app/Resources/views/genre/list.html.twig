<html>
  <head>
    <title>Liste des Genres</title>
  </head>
  <body>
    {% for genre in genres %}
    <p>{{ genre.label }}</p>
    <a href={{ path('genre_edit', {'id': genre.id}) }}>Modifier le genre {{ genre.label }}</a>
    <br />
    <a href={{ path('genre_delete', {'id': genre.id}) }}> Supprimer {{ genre.label }}</a>
    <hr />
    {% endfor %}
  </body>
</html>
